interface_build {
    {r.transect} 0
    {Outputs raster map layer values lying along user defined transect line(s).}
    {entry map {Raster map to be queried:} 0 raster}
    {entry line {Transect line definition(s) (east,north,azimuth,distance[,...]):} 0 +xy}
    {entry null {Character string to be used for no data value [*]:} 0 ""}
    {checkbox -g {Output Geographic Coordinates} "" -g}
}
