CC                  = @CC@
FC                  = @F77@
LEX                 = @LEX@
YACC                = yacc
ARCH                = @host@
MAKE                = make

GISBASE             = @prefix@
UNIX_BIN            = @BINDIR@

DEFAULT_DATABASE    =
DEFAULT_LOCATION    =

COMPILE_FLAGS       = @CFLAGS@ @INCLUDE_DIRS@
LDFLAGS             = @STRIPFLAG@ @LIBRARY_DIRS@

DLLIB               = @DLLIB@
XCFLAGS             = @X_CFLAGS@
# XXX - replace SYSV in code with configure results in config.h
# XXX - need SYSV defined for hpux10
XLDFLAGS            = @SYSV@
XINCPATH            = 
XMINCPATH           = 
XLIBPATH            = @X_LIBS@
XTLIBPATH           = 
XMLIBPATH           = 
XLIB                = @X_PRE_LIBS@ -lX11
XTLIB               = -lXt
XMLIB               = -lXm
XEXTRALIBS          = @X_EXTRA_LIBS@

COMPATLIB           = @COMPATLIB@
TERMLIB             = @TERMLIB@
CURSES              = @CURSESLIB@ $(TERMLIB) $(COMPATLIB)
MATHLIB             = -lm
XDRLIB              = @XDRLIB@

#PostgreSQL:
PQINCPATH           = @PQINCPATH@
PQLIBPATH           = @PQLIBPATH@
PQLIB               = @PQLIB@

#ODBC:
ODBCINC             = @ODBCINC@

#Image formats:
PNGINC              = -I/usr/X11R6/include/ -I/usr/include/gr -I/usr/local/include -I/usr/include
PNGLIB              = -L/usr/local/lib -L/usr/X11R6/lib -L/usr/lib

JPEGINCPATH         = -I/usr/X11R6/include/ -I/usr/include/gr -I/usr/local/include -I/usr/include
JPEGLIBPATH         = -L/usr/local/lib -L/usr/X11R6/lib -L/usr/lib

TIFFINCPATH         = @TIFF_INCLUDE_DIRS@
TIFFLIBPATH         = @TIFF_LIBRARY_DIRS@

#openGL files for NVIZ/r3.showdspf
OPENGLINC           = @OPENGLINC@
OPENGLwINC          = @OPENGLwINC@
OPENGLLIB           = @OPENGLLIB@
OPENGLULIB          = @OPENGLULIB@
LGLWM               = @LGLWM@

#tcl/tk stuff
TCLINCDIR           = @TCLINCDIR@
TKINCDIR            = @TKINCDIR@
TCLTKLIBS           = @TCLTKLIBS@

LIBRULE             = ar ruv $@ $?; @RANLIB@ $@
USE_TERMIO          = @USE_TERMIO@

