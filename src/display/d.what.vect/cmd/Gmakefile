PGM = d.what.vect
OFILES = \
	attr.o \
	main.o \
	openvect.o\
	what.o


all: $(BIN_CMD)/$(PGM)

LIBES=$(VECTLIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(DBMILIB)
DEPLIBS=$(DEPVECTLIB) $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPDBMILIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #
$(DEPVECTLIB): #
$(DEPDBMILIB): #
