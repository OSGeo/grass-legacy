PGM=d.db

OFILES = \
	point.o \
	main.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(DATETIMELIB) $(DBMILIB) $(DLGLIB)
DEPLIBS=$(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPDATETIMELIB) $(DEPDBMILIB) $(DEPDLGLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #
$(DEPDLGLIB): #
$(DEPDATETIMELIB): #
$(DEPDBMILIB): #

point.o: global.h
main.o: global.h
