CFILES = \
	compress.c \
	linecros.c \
	new_line.c \
	remove.c \
	break2.c \
	intersect.c \
	main2.c \
	spag2.c 

#  write_ascii.o in OFILES is just for debugging

B_A_OFILES = \
	ascii_io.o \
