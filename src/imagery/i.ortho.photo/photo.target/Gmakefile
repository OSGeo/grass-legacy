PROGRAM      = photo.target
HOME        = $(ETC)/imagery

EXTRA_CFLAGS = -I../libes
IMAGERYLIB2  = ../libes/libortho.a

LIST = main.o ask_target.o
LIBES=$(IMAGERYLIB2) $(IMAGERYLIB) $(GISLIB) $(VASKLIB)
DEPLIBS=$(IMAGERYLIB2) $(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB)

$(HOME)/$(PROGRAM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS) $(IMAGERYLIB2): #


