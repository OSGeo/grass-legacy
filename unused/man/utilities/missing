eval `gmake -sh`
bin=$GISBASE/bin

list=
for f in `cd $bin; ls`
do
    if [ ! -f help.src/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    echo The following commands do NOT have help entries
    (cd $bin; ls -dC $list) | sed s'/^/  /'
    echo ""
fi

list=
for f in `ls help.src`
do
    if [ ! -f $bin/$f ]
    then
	list="$list  help.src/$f"
    fi
done
if test "$list" != ""
then
    echo The following help entries do NOT have a command
    ls -dC $list | sed s'/^/  /'
    echo ""
fi

any=no
list=
for f in `cd $bin; ls [0-9a-z]*`
do
    if [ ! -f man1/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    any=yes
    echo The following manual entries are missing
    echo man1
    (cd $bin;ls -dC $list) | sed s'/^/  /'
fi

list=
for f in `cd $bin; ls [A-Z]*`
do
    if [ ! -f man2/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    if test $any = no
    then
	any=yes
	echo The following manual entries are missing
    fi
    echo man2
    (cd $bin;ls -dC $list) | sed s'/^/  /'
fi
if test $any = yes
then
    echo ""
fi

any=no
list=
for f in `ls man1`
do
    if [ ! -f $bin/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    any=yes
    echo The following manual entries do NOT have a command
    echo man1
    (cd man1;ls -dC $list) | sed s'/^/  /'
fi

list=
for f in `ls man2`
do
    if [ ! -f $bin/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    if test $any = no
    then
	any=yes
	echo The following manual entries do NOT have a command
    fi
    echo man2
    (cd man2;ls -dC $list) | sed s'/^/  /'
fi
if test $any = yes
then
    echo ""
fi

any=no
list=
for f in `ls cat1`
do
    if [ ! -f man1/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    any=yes
    echo The following cat entries do not have an man entry
    echo cat1
    (cd cat1;ls -dC $list) | sed s'/^/  /'
fi

list=
for f in `ls cat2`
do
    if [ ! -f man2/$f ]
    then
	list="$list $f"
    fi
done
if test "$list" != ""
then
    if test $any = no
    then
	any=yes
	echo The following cat entries do not have an man entry
    fi
    echo cat2
    (cd cat2;ls -dC $list) | sed s'/^/  /'
fi
if test $any = yes
then
    echo ""
fi
