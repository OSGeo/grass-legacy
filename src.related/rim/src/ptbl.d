C
C     *** PTBL ***
C
C     POINTER TABLE FOR ALGEBRA COMMANDS
C
C        PTBL1 -- ATTRIBUTE NAME
C        PTBL2 -- ATTRIBUTE LOCATION IN RELATION 1
C        PTBL3 -- ATTRIBUTE LOCATION IN RELATION 2
C        PTBL4 -- ATTRIBUTE LOCATION IN RELATION 3
C        PTBL5 -- LENGTH IN WORDS
C        PTBL6 -- ATTRIBUTE TYPE
C        PTBL7 -- ATTRIBUTE FORMAT
C
      PARAMETER (PTBL1=1,PTBL2=PTBL1+Z,PTBL3=PTBL2+1,PTBL4=PTBL3+1)
      PARAMETER (PTBL5=PTBL4+1,PTBL6=PTBL5+1,PTBL7=PTBL6+1)
      PARAMETER (PTBLL=PTBL7)
C
