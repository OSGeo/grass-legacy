            for dir in $tk_try; do
                if test -z "$TCLINCDIR" && test -d "$my_dir/$dir" && test -f "$my_dir/$dir/tcl.h"; then
                    grep_cmd="grep TCL_VERSION ${my_dir}/${dir}/tcl.h"
                    tcltk_ver=`$grep_cmd | sed -e 's/^\(.*\)\"\(.*\)\"$/\2/'`
                    TCLINCDIR="-I${my_dir}/${dir}"
