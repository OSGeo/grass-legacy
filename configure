            if test -z "$TCLINCDIR" && test -f "$my_dir/tcl.h"; then
                grep_cmd="grep TCL_VERSION ${my_dir}/tcl.h"
                tcltk_ver=`$grep_cmd | sed -e 's/^\(.*\)\"\(.*\)\"$/\2/'`
                TCLINCDIR="-I${my_dir}"
